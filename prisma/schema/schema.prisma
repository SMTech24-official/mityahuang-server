generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model ExchangeRate {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  from String // the currencies in short form --> CNY
  to   String // the currencies in short form --> USD
  rate Float

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("exchange-rate")
}
